_ERROR, \n        message: error\n    }\n}\n\nexport const removeError = () => {\n    return {\n        type: REMOVE_ERROR\n    }\n}\n\n// THUNKS\n\n// REDUCER - generic error handler\n\nexport default (state: ErrorState = { message: null}, action: Actions) => {\n    switch (action.type) {\n        case ADD_ERROR:\n            return {...state, message: action.message};\n        case REMOVE_ERROR:\n            return {...state, message: null};\n        default:\n            return state;\n    }\n}","/home/qsmit/hot-off-the-press/client/src/components/AuthForm.tsx",[],"/home/qsmit/hot-off-the-press/client/src/services/api.tsx",[],"/home/qsmit/hot-off-the-press/client/src/components/pages/NewsFeed.tsx",["75","76","77","78","79"],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":18,"column":10,"nodeType":"86","messageId":"87","endLine":18,"endColumn":14},{"ruleId":"84","severity":1,"message":"88","line":18,"column":16,"nodeType":"86","messageId":"87","endLine":18,"endColumn":23},{"ruleId":"84","severity":1,"message":"89","line":2,"column":37,"nodeType":"86","messageId":"87","endLine":2,"endColumn":45},{"ruleId":"84","severity":1,"message":"90","line":2,"column":47,"nodeType":"86","messageId":"87","endLine":2,"endColumn":66},{"ruleId":"84","severity":1,"message":"91","line":35,"column":6,"nodeType":"86","messageId":"87","endLine":35,"endColumn":26},{"ruleId":"92","severity":1,"message":"93","line":102,"column":1,"nodeType":"94","endLine":113,"endColumn":2},{"ruleId":"95","severity":1,"message":"96","line":25,"column":25,"nodeType":"97","endLine":25,"endColumn":119},{"ruleId":"95","severity":1,"message":"96","line":26,"column":25,"nodeType":"97","endLine":26,"endColumn":117},{"ruleId":"95","severity":1,"message":"96","line":27,"column":25,"nodeType":"97","endLine":27,"endColumn":119},{"ruleId":"95","severity":1,"message":"96","line":28,"column":25,"nodeType":"97","endLine":28,"endColumn":119},{"ruleId":"